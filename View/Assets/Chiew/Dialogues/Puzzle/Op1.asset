  - isBubble: 0
    TextColor: {r: 0, g: 0, b: 0, a: 1}
    characImage: {fileID: 0}
    nameText: Alex
    DialogueText: "No...no. You\u2019re not me. You don\u2019t know what I\u2019ve
      been through, what I'm going...through. You know nothing at all. "
    isEndedSentence: 0
    isPlayed: 0
    isQuestion: 0
